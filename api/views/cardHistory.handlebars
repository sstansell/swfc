{{debug}}
<h1>History for {{{data.card.name}}}</h1>
<div class="cardInfo">
	<img class="cardImage" src="http://amw.konaminet.jp/amw/naboo/img/{{data.card.thumb}}_m.png">
	<ul>
	<li>Appear Date: {{data.card.appear_date}}</li>
	<li>Last Update: {{data.card.last_update}}</li>
	</ul>
</div>

<table>
<caption>All dates determined by UTC</caption>
	<thead>
		<tr>
			<th>Date</th>
			<th>Count</th>
			<th>% of Total Pulls</th>
		</tr>
	</thead>
	<tbody>
		{{#each data.days}}
		<tr>
			<td>{{date}}</td>
			<td>{{count}}</td>
			<td></td>
		</tr>
		{{/each}}
		<tr>
			<td>Total</td>
			<td>{{data.total}}</td>
			<td></td>
		</tr>
	</tbody>
</table>

