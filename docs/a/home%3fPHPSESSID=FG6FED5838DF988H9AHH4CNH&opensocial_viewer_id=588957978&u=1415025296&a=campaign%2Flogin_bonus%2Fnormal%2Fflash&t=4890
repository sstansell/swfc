			} else {
				$('#caps_card_attack').html("ATK: ");
			}
		});
		function newsTicker(targetArea){
			var i = 0;
			var children = $(targetArea).children();
			var iTotal = children.length;
			var containerWidth	= $(targetArea).width();
			var contentWidth	= $(children[i]).width();
			var totalDist	= containerWidth + contentWidth;
			var totalSpeed	= totalDist / 10;
			pos = 0;
			$(children[i]).css({"display":"inline-block"});
			setInterval(
				function newsTick(){
					if(pos >= (totalDist + 20)){
						if(iTotal == (i + 1)){
							i = 0;
						}else{
							i ++;
						}
						$(children).css({"display":"none"});
						$(children[i]).css({"display":"inline-block"});
						contentWidth	= $(children[i]).width();
						totalDist	= containerWidth + contentWidth;
						$(children[i]).css({"-webkit-transform":"translate(" + totalDist + "px,0)"});
						pos = 0;
					}else{
						pos += 1;
						$(children[i]).css({"-webkit-transform":"translate(-" + pos + "px,0)"});
					}
				}
			,totalSpeed);
		}
		new newsTicker("#marquee_area");
		$("#news_dialog").implant_dialog({closer:".close"});
		$("#sw-content").fadeIn();
    });
	function openBannerDialog(){
		$("#banner_list_dialog").call_implant_dialog();
	}
	function openNewsDialog(){
		$("#news_dialog").call_implant_dialog();
	}
</script>
