<script type="text/javascript" src="http://amw.konaminet.jp/amw/naboo/js/jquery/plugin/countDown.js?2014103001"></script>
<script type="text/javascript">

				
	$(document).ready(function(){
		
		$('#dialog_news_action_result').implant_dialog({closer:'.modal_close'});
			}); 
    function homeAjax(){
        this.queue_count = 0;
        this.queue       = {};
        this.queue_index = 0;
    };
    homeAjax.prototype={
        addQueue:function(url, id, type, callback){
            this.queue[this.queue_index] = {
                'url':url,
                'id':id,
                'type':type,
                'callback':callback
            };
            this.queue_index++;
        },
        homeAjaxLoad:function(){
            var that = this;
            if(that.queue_count == that.queue_index){
                return;
            }
            url      = that.queue[that.queue_count]['url'];
            id       = that.queue[that.queue_count]['id'];
            type     = that.queue[that.queue_count]['type'];
            callback = that.queue[that.queue_count]['callback'];
            
            $.ajax({
                type:'POST',
                url:url,
                dataType:type,
                data:{},
                timeout:300000,
                error:function(XMLHttpRequest,textStatus,errorThrown){
                }
            }).then(function(data){
                if(callback == undefined){
                    $("#"+id).empty();
                    $("#"+id).html(data);
                }else{
                    callback(data);
                }
                that.queue_count++;
                that.homeAjaxLoad();
            });
        }
    }
</script>

