			function user_grade_update() {
									GREE.ui(
						{
							'method':'upgrade',
							'grade':3,
							'callback_url':'http://amw.konaminet.jp/amw/naboo/content_viewer/initialize?PHPSESSID=FG6FED5838DF988H9AHH4CNH&opensocial_viewer_id=588957978&u=1415025307'
						},
						function(response){
							if(response.data.upgraded) {
			
								top.location.href = "http://amw.konaminet.jp/amw/naboo/content_viewer/initialize?PHPSESSID=FG6FED5838DF988H9AHH4CNH&opensocial_viewer_id=588957978&u=1415025307";
							}
						}
					);
							}
	
										sendMessageToApp('session.start', {id:'FG6FED5838DF988H9AHH4CNH', 'ua':navigator.userAgent});
									sendMessageToApp('menu.setdata', {'value':'http://amw.konaminet.jp/amw/naboo/', 'name':'BASE_URL'});
						$(window).resize(function(){
				var WINDOW_HEIGHT	= $("#sw-content").height();
				$("#sw-content").css({"height": getWVHeight() +"px"});
			});
