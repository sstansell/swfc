							$("#sw-content").css({"height": getWVHeight() +"px"});
											}
				
				$(".btnN.blink, .btnS.blink, .btnSS.blink").append("<div class='whiteBlink'></div>");
				
				
								
									/*sendMessageToApp('menu.show');*/
                                            sendMessageToApp('menu.show', {'battle_lock':0});
                    				                sendMessageToApp('page.scroll');
			});	
