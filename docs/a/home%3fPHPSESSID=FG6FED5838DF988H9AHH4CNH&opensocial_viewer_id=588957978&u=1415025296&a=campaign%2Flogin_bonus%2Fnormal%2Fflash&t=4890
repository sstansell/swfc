<script type="text/javascript">
    var ajaxLock = 0;
    function get_ajax_data(url, target){
        if(ajaxLock) return;
        
        ajaxLock = 1;
        var that = this;
        $.ajax({
            type:'POST',
            url:url,
            dataType:'json',
            data:{},
            timeout:300000,
            error:function(XMLHttpRequest,textStatus,errorThrown){
				$('.dialog_news_action_message').html("ERROR");
				$('#dialog_news_action_result').call_implant_dialog();
				ajaxLock = 0;
            }
        }).then(function(data){
            if(data.code == 200){
                console.log("OK");
                var current_news_num = $('.news_badge_num').html();
                var result_news_num = current_news_num - 1;
                if(result_news_num > 0){
                	$('.news_badge_num').html(result_news_num);
                	$('.news_each_'+ target).css({"display":"none"});
                }else{
                	$('.news_badge_num').css({"display":"none"});
                	$('.news_each_'+ target).html("<div class='pad15 txtAC no-news'>No new information available</div>");
                	$('#news_wrapper').attr("onclick","location.href='http://amw.konaminet.jp/amw/naboo/home/menu_news?PHPSESSID=FG6FED5838DF988H9AHH4CNH&opensocial_viewer_id=588957978&u=1415025307'");
                }
                console.log(current_news_num);
                console.log(result_news_num);
                					$('#news_dialog').close_implant_dialog();
                                $('.dialog_news_action_message').html(data.message);
                $('#dialog_news_action_result').call_implant_dialog();
            } else {
                $('.dialog_news_action_message').html(data.message);
                $('#dialog_news_action_result').call_implant_dialog();
            }
            ajaxLock = 0;
        });
    }
</script>
