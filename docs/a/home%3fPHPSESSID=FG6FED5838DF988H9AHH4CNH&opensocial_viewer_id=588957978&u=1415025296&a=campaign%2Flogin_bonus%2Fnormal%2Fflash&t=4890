			}
			
			
			var push_back="";
			var onclick_back="";
			function setPushBack(back){
				push_back = back;
				console.log(push_back);
			}
			function pushBack(){
							var url = location.href;
				if(is_redirect){
					
					document.location = redirect_url;
				}else if(url.indexOf("card/deck/list") >= 0){
					document.location= "http://amw.konaminet.jp/amw/naboo/card/deck/list?PHPSESSID=FG6FED5838DF988H9AHH4CNH&opensocial_viewer_id=588957978&u=1415025307";
				}else if(is_page_reload){
					
					document.location.reload();
				}else if(is_dialog_open){
					
					if(dialog_target.length >= 2){
						document.location.reload();
					}else{
						$(dialog_target[dialog_target.length-1]).close_implant_dialog();
					}
				}else{
					
					if(url.indexOf("home/home") == -1){
						if(url.indexOf("help/user_policy") >= 0){
							
							   sendMessageToApp('page.title');
						}else{
							if(push_back != ""){
								
								document.location = push_back;
							}else if(onclick_back != ""){
								
								onclick_back();
							}else{
															document.location= "http://amw.konaminet.jp/amw/naboo/home/home?PHPSESSID=FG6FED5838DF988H9AHH4CNH&opensocial_viewer_id=588957978&u=1415025307";
														}
						}
					}else{
						sendMessageToApp('app.quitDialog');
					}
				}
						}
